<Page x:Class="IMS.Client.WPF.EquipPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:IMS.Client.WPF"
      mc:Ignorable="d" 
      d:DesignHeight="1020" d:DesignWidth="1840"
      Title="ListPage">

    <Grid>
        <Rectangle x:Name="SubMenu" Fill="#FFF4F4F5" HorizontalAlignment="Left" Stroke="Black" Width="160"/>
        <Button x:Name="button_ups_setting" Content="UPS setting" HorizontalAlignment="Left" VerticalAlignment="Top" Width="160" Height="80" Click="button_ups_setting_Click"/>
        <Button x:Name="button_add_instrument" Content="Add instrument" HorizontalAlignment="Left" Margin="0,80,0,0" VerticalAlignment="Top" Width="160" Height="80" Click="button_add_instrument_Click"/>
        <Button x:Name="button_panel_setting" Content="Panel setting" HorizontalAlignment="Left" Margin="0,160,0,0" VerticalAlignment="Top" Width="160" Height="80" Click="button_panel_setting_Click"/>
        <Frame x:Name="MainFrame" Content="" Margin="160,0,0,0"/>
    </Grid>
</Page>
