        <Label x:Name="label1" Content="온도" HorizontalAlignment="Left" Margin="1498,79,0,0" VerticalAlignment="Top" Height="41" Width="124" FontSize="26.667" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
