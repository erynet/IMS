<Page x:Class="IMS.Client.WPF.LogViewPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:IMS.Client.WPF"
      mc:Ignorable="d" 
      d:DesignHeight="760" d:DesignWidth="800"
      Title="LogViewPage">

    <Grid>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="198" Margin="12,67,0,0" Stroke="Black" VerticalAlignment="Top" Width="252"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="596" Margin="556,67,0,0" Stroke="Black" VerticalAlignment="Top" Width="209"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="198" Margin="287,67,0,0" Stroke="Black" VerticalAlignment="Top" Width="251"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="198" Margin="12,282,0,0" Stroke="Black" VerticalAlignment="Top" Width="252"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="198" Margin="287,282,0,0" Stroke="Black" VerticalAlignment="Top" Width="251"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="199" Margin="12,499,0,0" Stroke="Black" VerticalAlignment="Top" Width="252"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="199" Margin="287,499,0,0" Stroke="Black" VerticalAlignment="Top" Width="251"/>
        <Button x:Name="button_excel_export" Content="엑셀 출력" HorizontalAlignment="Left" Margin="556,672,0,0" VerticalAlignment="Top" Width="85" Click="button_excel_export_Click"/>
        <Label x:Name="label1" Content="온도" HorizontalAlignment="Left" Margin="641,33,0,0" VerticalAlignment="Top" Height="30" Width="55" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Button x:Name="button_digital" Content="디지털" HorizontalAlignment="Left" Margin="690,13,0,0" VerticalAlignment="Top" Width="75" Height="20" Click="button_digital_Click"/>
        <Label x:Name="label2" Content="장비 번호&#xA; :" HorizontalAlignment="Left" Margin="12,10,0,0" VerticalAlignment="Top" Width="72" Height="26" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <TextBox x:Name="equipment_number" HorizontalAlignment="Left" Height="26" Margin="89,10,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Button x:Name="button_close" Content="닫기" HorizontalAlignment="Left" Margin="680,672,0,0" VerticalAlignment="Top" Width="85" Height="20" Click="button_close_Click"/>
    </Grid>
</Page>
