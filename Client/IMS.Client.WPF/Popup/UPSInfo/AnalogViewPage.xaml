<Page x:Class="IMS.Client.WPF.AnalogViewPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:IMS.Client.WPF"
      mc:Ignorable="d" 
      d:DesignHeight="760" d:DesignWidth="800"
      Title="AnalogViewPage">

    <Grid>

        <Button x:Name="button_digital" Content="디지털" HorizontalAlignment="Left" Margin="690,13,0,0" VerticalAlignment="Top" Width="75" Height="20" Click="button_digital_Click"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="316" Margin="12,63,0,0" Stroke="Black" VerticalAlignment="Top" Width="382"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="316" Margin="408,63,0,0" Stroke="Black" VerticalAlignment="Top" Width="357"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="226" Margin="12,436,0,0" Stroke="Black" VerticalAlignment="Top" Width="120"/>
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="226" Margin="408,436,0,0" Stroke="Black" VerticalAlignment="Top" Width="357"/>
        <Button x:Name="button_close" Content="닫기" HorizontalAlignment="Left" Margin="680,672,0,0" VerticalAlignment="Top" Width="85" Height="20" Click="button_close_Click"/>
        <Button x:Name="button_see_log" Content="로그 보기" HorizontalAlignment="Left" Margin="586,672,0,0" VerticalAlignment="Top" Width="85" Height="20" Click="button_see_log_Click"/>
        <Button x:Name="button_error_list" Content="고장 목록" HorizontalAlignment="Left" Margin="493,672,0,0" VerticalAlignment="Top" Width="85" Height="20" Click="button_error_list_Click"/>
        <Label x:Name="label1" Content="입력" HorizontalAlignment="Left" Margin="24,46,0,0" VerticalAlignment="Top" Width="86" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF9E9E9E"/>
        <Label x:Name="label1_Copy" Content="입력" HorizontalAlignment="Left" Margin="423,46,0,0" VerticalAlignment="Top" Width="85" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF9E9E9E"/>
        <Label x:Name="label1_Copy1" Content="배터리" HorizontalAlignment="Left" Margin="24,427,0,0" VerticalAlignment="Top" Width="86" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF9E9E9E"/>
        <Label x:Name="label1_Copy2" Content="출력" HorizontalAlignment="Left" Margin="423,423,0,0" VerticalAlignment="Top" Width="85" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF9E9E9E"/>
        <Label x:Name="label2" Content="장비 번호&#xA; :" HorizontalAlignment="Left" Margin="12,10,0,0" VerticalAlignment="Top" Width="72" Height="26" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <TextBox x:Name="equipment_number" HorizontalAlignment="Left" Height="26" Margin="89,10,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>

    </Grid>
</Page>
